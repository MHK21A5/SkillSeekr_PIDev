{% extends 'Backbase.html.twig' %}


{% block title%}Reclamation{% endblock%}

{% block body %}
<div class="container">

        
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Content</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
  {% for data in list %}
    <tr>
      <th scope="row">{{data.idrec}}</th>
      <td>{{data.title}}</td>
      <td>{{data.content}}</td>
      <td>
      <div class="container-md">
      <a class="btn btn-danger" href={{ path('backdelete',{'idrec':data.idrec}) }}">Delete</a>
        </div>
      </td>
      {% endfor %}
    </tr>
  </tbody>
</table>
{% endblock %}